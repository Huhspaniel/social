{
  "name": "social",
  "version": "0.1.0",
  "description": "Social networking application built using AWS, PostgreSQL, Node.js, Express, and Angular",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon --ignore client",
    "client": "cd client && npm run start -- -o",
    "ng": "cd client && ng",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Huhspaniel/social.git"
  },
  "author": "Huhspaniel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Huhspaniel/social/issues"
  },
  "homepage": "https://github.com/Huhspaniel/social#readme",
  "dependencies": {
    "bcrypt": "^3.0.3",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "pg": "^7.7.1",
    "pg-hstore": "^2.3.2",
    "ramda": "^0.26.1",
    "sequelize": "^4.42.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.9"
  }
}
